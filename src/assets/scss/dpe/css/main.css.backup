/*-------------------Header------------------*/
header {
  background: url("../img/nav_bg.gif") repeat-x 0px 0px;
  height: 127px;
}

header .logo-area {
  float: left;
  width: 11%;
}

header .logo {
  width: 130px;
}

header .menu-area {
  float: left;
  width: 89%;
}

header #nav-header {
  margin-top: 10px;
  height: 40px;
}

header #nav-header div {
  float: left;
  color: #fff;
}

header .circle, header .circle-gray, header .circle-black {
  width: 30px;
  height: 30px;
  cursor: pointer;
  color: #fff;
  -webkit-border-radius: 25px;
  -moz-border-radius: 25px;
  border-radius: 25px;
}

header .circle:hover, header .circle-gray:hover, header .circle-black:hover {
  background-color: #208118;
  color: #fff;
}

header .circle-gray {
  background: #b5b5b5;
  padding: 4px 6px;
  font-family: 'hel55';
  font-weight: normal;
  letter-spacing: 0.5px;
  font-size: 17px;
}

header .circle-gray else {
  font-size: 19px;
}

header .circle-black {
  padding: 4px 6px;
  background: #333;
  font-family: 'hel55';
  font-weight: normal;
  letter-spacing: 0.5px;
  font-size: 17px;
}

header .circle-black else {
  font-size: 19px;
}

header .circle-active {
  background: #333;
}

header .circle-font, header #circle-font1, header #circle-font2, header #circle-font3 {
  width: 20px;
  height: 20px;
  cursor: pointer;
  padding-left: 5px;
  color: #014a36;
  -webkit-border-radius: 25px;
  -moz-border-radius: 25px;
  border-radius: 25px;
}

header .circle-font:hover, header #circle-font1:hover, header #circle-font2:hover, header #circle-font3:hover {
  background-color: #208118;
}

header #circle-font1 {
  margin: 2px;
  margin-left: 10px;
  padding-left: 7px;
  background: #333;
}

header #circle-font2 {
  margin: 2px;
  line-height: 18px;
  padding-left: 6px;
  background: #333;
}

header #circle-font3 {
  margin: 2px;
  line-height: 18px;
  background: #333;
}

